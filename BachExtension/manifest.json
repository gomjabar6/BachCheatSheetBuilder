{
    "name": "Bachelor Bracket CheatSheet Builder",
    "description": "Creates a cheat sheet from bachbracket.com!",
    "version": "1.1",
    "manifest_version": 3,
    "content_scripts": [
        {
            "matches": [
                "https://*.bachbracket.com/leagues/*/bracket_picks*"
            ],
            "js": [
                "thirdParty/jquery-3.6.0.min.js",
                "extension.js"
            ]
        }
    ]
}